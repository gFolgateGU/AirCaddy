<!--Upload Modal-->
<div class="modal fade" id="reasonPopUp" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                    &times;
                </button>
                <h4 class="modal-title" id="myModalLabel">
                    Upload Footage: <span data-bind="text: courseName"></span> <strong>(Hole <span data-bind="text: holeInFocus"></span>)</strong>
                </h4>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-xs-12">
                        <input data-bind="event: { change: function() { uploadVideo($element.files[0]) } }" type="file"/>                       
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button id="cancelUploadBtn" type="button" class="btn btn-default" data-dismiss="modal">OK</button>
                <button data-bind="click: submitUpload" id="uploadFootageBtn" type="button" class="btn btn-primary">UPLOAD</button>
            </div>
        </div>
    </div>
</div>
